        def contained(self, pattern, x):                                                                                                                                  
                print "Pattern: %s, x: %s" % (pattern, x)                                                                                                                 
                if(x.count(pattern) > 0):                                                                                                                                 
                        return 1                                                                                                                                          
                else:                                                                                                                                                     
